{{#with emoji_dict}}
<div class="emoji-popover-emoji {{#if has_reacted}}reacted{{/if}}" data-emoji-name="{{name}}" tabindex="0" data-emoji-id="{{../type}},{{../section}},{{../index}}">
    {{#if is_realm_emoji}}
    <img src="{{url}}" class="emoji"/>
    {{else}}
    <div class="emoji emoji-{{emoji_code}}"></div>
    {{/if}}
</div>
{{/with}}
