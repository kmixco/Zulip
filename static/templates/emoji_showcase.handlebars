{{#with emoji_dict}}
<div class="emoji-showcase">
    {{#if is_realm_emoji}}
    <img src="{{url}}" class="emoji emoji-preview"/>
    {{else}}
    <div class="emoji emoji-preview emoji-{{emoji_code}}"></div>
    {{/if}}
    <div class="emoji-canonical-name" title="{{display_name}}">{{display_name}}</div>
</div>
{{/with}}
